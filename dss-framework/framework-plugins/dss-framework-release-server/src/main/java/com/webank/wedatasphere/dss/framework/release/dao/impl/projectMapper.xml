<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.webank.wedatasphere.dss.framework.release.dao.ProjectMapper">

    <resultMap id="projectInfo" type="com.webank.wedatasphere.dss.framework.release.entity.project.ProjectInfo">
        <id column="name" property="projectName" jdbcType="VARCHAR"/>
        <result column="id" property="projectId" jdbcType="BIGINT"/>
        <result column="create_by" property="createBy" jdbcType="LONGVARCHAR"/>
        <result column="update_by" property="updateBy" jdbcType="LONGVARCHAR"/>
        <result column="create_time" property="createTime" jdbcType="TIMESTAMP"/>
        <result column="update_time" property="updateTime" jdbcType="TIMESTAMP"/>
        <result column="description" property="description" jdbcType="LONGVARCHAR"/>
    </resultMap>



    <select id = "getProjectInfoById" resultMap="projectInfo" parameterType="java.lang.Long">
        select * from dss_project where id = #{projectId}
    </select>


    <update id = "updateProjectOrcInfo" parameterType="java.lang.Long">
        update dss_project_orchestrator set `orchestrator_version_id` = #{orchestratorVersionId}
        where `project_id` = #{projectId} and `orchestrator_id` = #{orchestratorId}
    </update>







</mapper>